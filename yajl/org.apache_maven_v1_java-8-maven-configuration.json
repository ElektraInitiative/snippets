{
    "artifactId": "spaces-base",
    "build": {
        "pluginManagement": {
            "plugins": {
                "plugin": {
                    "artifactId": "maven-compiler-plugin",
                    "configuration": {
                        "source": "${java.version}",
                        "target": "${java.version}"
                    },
                    "groupId": "org.apache.maven.plugins",
                    "version": "3.6.0"
                }
            }
        }
    },
    "groupId": "at.ac.tuwien.spaces",
    "modelVersion": "4.0.0",
    "properties": {
        "java.version": "1.8"
    },
    "version": "0.0.1-SNAPSHOT"
}
