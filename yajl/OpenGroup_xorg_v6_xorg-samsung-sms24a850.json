[
    "Section \"Monitor\"",
    "    Identifier     \"Monitor0\"",
    "    VendorName     \"Samsung \"",
    "    ModelName      \"SMS24A850\"",
    "#Monitors have: (not used, because does not work with Beamer)",
    "#  HorizSync   : 30.000-81.000 kHz",
    "#  VertRefresh : 56.000-75.000 Hz",
    "#",
    "#Monitor EDID said:",
    "#  Pixel Clock      : 154.00 MHz",
    "#  HRes, HSyncStart : 1920, 1968",
    "#  HSyncEnd, HTotal : 2000, 2080",
    "#  VRes, VSyncStart : 1200, 1203",
    "#  VSyncEnd, VTotal : 1209, 1235",
    "#  H/V Polarity     : +/-",
    "#",
    "# This would specify everything and completely disable EDID",
    "# It does not work, however, because a native panel with very",
    "# low resultion is assumed then...",
    "    #ModeLine       \"1920x1200\" 154.00 1920 1968 2000 2080 1200 1203 1209 1235 +hsync -vsync",
    "    #Option \"ModeValidation\" \"NoEdidModes\"",
    "",
    "",
    "#Beamer EDID said:",
    "# Pixel Clock      : 74.25 MHz",
    "# HRes, HSyncStart : 1280, 1390",
    "# HSyncEnd, HTotal : 1430, 1650",
    "# VRes, VSyncStart : 720, 725",
    "# VSyncEnd, VTotal : 730, 750",
    "# H/V Polarity : +/+",
    "#The new modeline (based on above EDID) for the beamer:",
    "    ModeLine       \"1280x720\" 74.25 1280 1390 1430 1650 720 725 730 750 +hsync +vsync",
    "#Beamer has (from xorg.log output)",
    "#    HorizSync       31.0 - 64.0",
    "#    VertRefresh     50.0 - 120.0",
    "#But beamer says by message on too high sync",
    "#that it only has maximum of: fH=74KHz, fV=60Hz",
    "# the minimum values are taken from Beamer (31) and Samsung (56)",
    "# Note: UseEDIDFreqs is needed, otherwise those values are ignored",
    "#    HorizSync       31.0 - 73.99",
    "#    VertRefresh     56.0 - 60.0",
    "    Option \"HorizSync\"   \"DFP-0: 31-73.99;  DFP-1: 31-73.99\"",
    "    Option \"VertRefresh\" \"DFP-0: 56-60;  DFP-1: 56-60\"",
    "    Option \"UseEDIDFreqs\" \"FALSE\"",
    "",
    "    Option \"CustomEDID\" \"DFP-0:/etc/X11/monitor.edid; DFP-1:/etc/X11/monitor.edid\"",
    "",
    "#  Display Size not needed because Monitor directly connected:",
    "#   DisplaySize     520 320",
    "    Option         \"DPMS\"",
    "EndSection"
]
